ARCH    := i386
TARGET  := i386-jos-linux
OBJTYPE := elf32-i386

CC      := ft-cc
CXX     := $(GCCPREFIX)g++ -pipe -m32
AS      := $(GCCPREFIX)as --32
AR      := $(GCCPREFIX)ar
LD      := $(GCCPREFIX)ld -m elf_i386
OBJCOPY := $(GCCPREFIX)objcopy
OBJDUMP := $(GCCPREFIX)objdump -m i386
NM      := $(GCCPREFIX)nm
STRIP   := $(GCCPREFIX)strip
