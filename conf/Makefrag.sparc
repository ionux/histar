ARCH    := sparc
TARGET  := sparc-jos-linux
OBJTYPE := elf32-sparc
BFDARCH := sparc

CC      := $(GCCPREFIX)gcc -pipe -m32 -msoft-float
CXX     := $(GCCPREFIX)g++ -pipe -m32 -msoft-float
AS      := $(GCCPREFIX)as -32
AR      := $(GCCPREFIX)ar
LD      := $(GCCPREFIX)ld -m elf32_sparc
OBJCOPY := $(GCCPREFIX)objcopy
OBJDUMP := $(GCCPREFIX)objdump -m sparc
NM      := $(GCCPREFIX)nm
STRIP   := $(GCCPREFIX)strip
