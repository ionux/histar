OBJDIRS += pkg/rild/support

RILD_SUPPORT_SRCFILES := Parcel.cpp SharedBuffer.cpp String16.cpp String8.cpp record_stream.cpp
RILD_SUPPORT_OBJFILES := $(patsubst %.cpp, $(OBJDIR)/pkg/rild/support/%.o, $(RILD_SUPPORT_SRCFILES))

$(OBJDIR)/pkg/rild/support/%.o: pkg/rild/support/%.cpp
	@mkdir -p $(@D)
	$(CXX) $(USER_CXXFLAGS) -Werror -Wno-missing-noreturn -c -o $@ $<
