OBJDIRS += pkg/htcdream/librpc

HTCDREAM_LIBRPC_SRCFILES := clnt.c ops.c rpc.c svc.c svc_clnt_common.c xdr.c
HTCDREAM_LIBRPC_OBJFILES := $(patsubst %.c, $(OBJDIR)/pkg/htcdream/librpc/%.o, $(HTCDREAM_LIBRPC_SRCFILES))

$(OBJDIR)/pkg/htcdream/librpc/%.o: pkg/htcdream/librpc/%.c
	@mkdir -p $(@D)
	$(CXX) $(USER_CXXFLAGS) -Werror -Wno-missing-noreturn -c -o $@ $<
