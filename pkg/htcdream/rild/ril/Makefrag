OBJDIRS += pkg/htcdream/rild/ril

RILD_RIL_SRCFILES := ril.cpp ril_event.cpp
RILD_RIL_OBJFILES := $(patsubst %.cpp, $(OBJDIR)/pkg/htcdream/rild/ril/%.o, $(RILD_RIL_SRCFILES))

$(OBJDIR)/pkg/htcdream/rild/ril/%.o: pkg/htcdream/rild/ril/%.cpp
	@mkdir -p $(@D)
	$(CXX) $(USER_CXXFLAGS) -Werror -Wno-unused -Wno-missing-noreturn -c -o $@ $<
