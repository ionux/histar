OBJDIRS += pkg/uclibc/libc/pwd_grp

PWD_GRP_MSRC := pkg/uclibc/libc/pwd_grp/pwd_grp.c
PWD_GRP_MOBJ := \
	fgetpwent_r.o fgetgrent_r.o fgetpwent.o fgetgrent.o \
        getpwnam_r.o getgrnam_r.o getpwuid_r.o getgrgid_r.o \
        getpwuid.o getgrgid.o getpwnam.o getgrnam.o getpw.o \
        getpwent_r.o getgrent_r.o getpwent.o getgrent.o \
        initgroups.o putpwent.o putgrent.o \
        __parsepwent.o __parsegrent.o __pgsreader.o
PWD_GRP_MOBJ := $(patsubst %.o, $(OBJDIR)/pkg/uclibc/libc/pwd_grp/%.o, $(PWD_GRP_MOBJ))
LIBC_OBJFILES += $(PWD_GRP_MOBJ)

$(PWD_GRP_MOBJ): $(PWD_GRP_MSRC)
	@mkdir -p $(@D)
	$(CC) $(UCLIBC_CFLAGS) -DL_$(patsubst $(OBJDIR)/pkg/uclibc/libc/pwd_grp/%.o,%,$@) $< -c -o $@

